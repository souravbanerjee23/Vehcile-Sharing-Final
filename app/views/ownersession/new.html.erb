<div class="d-flex align-items-center justify-content-center">
<h1>Owner Sign-IN</h1><br>
</div>
<%= form_with url: '/login/owners' do |form| %>
<div class="mb-3">
<%= form.label:email %>
<%= form.text_field:email ,class:"form-control",placeholder:"souravbandyopadhyay1997@gmail.com"%>
</div>
<div class="mb-3">
<%= form.label:password %>
<%= form.password_field:password,class:"form-control" ,placeholder:"password"%>
</div>
<div class="mb-3">
  <%= form.submit %>
</div>
<% end %>